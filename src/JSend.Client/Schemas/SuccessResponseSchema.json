{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "description": "Schema for validating a JSend response with success status",
    "type": "object",
    "properties": {
        "status": {
            "type": "string",
            "required": true,
            "enum": [ "success" ]
        },
        "data": {
            "required": true
        }
    }
}